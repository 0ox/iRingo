#!name= Response Geo Services to 🇺🇸US
#!desc=(BETA) 只作用于检测地区，不修改Apple Maps和Apple News，可以通过搭配使用其他模块达成混合效果。

[Script]
# Response Geo Services to US
http-request ^https?:\/\/gspe\d*-ssl\.ls\.apple\.com(\/pep\/gcc) script-path=https://raw.githubusercontent.com/VirgilClyne/iRingo/main/js/Geo_Services.js, argument=GeoCountryCode=US, timeout=10, tag=Geo_Services
# Redirect Geo Services Configuration Functions
http-response ^https?:\/\/configuration\.ls\.apple\.com\/config\/defaults\?.* script-path=https://raw.githubusercontent.com/VirgilClyne/iRingo/main/js/Geo_Services.js, argument=EnableAlberta=false&GEOAddressCorrectionEnabled=true&LocalitiesAndLandmarksSupported=true&PedestrianAREnabled=true&ShouldEnableLagunaBeach=true, requires-body=true, timeout=10, tag=Geo_Services

[MITM]
hostname = %APPEND% configuration.ls.apple.com, gspe1-ssl.ls.apple.com
